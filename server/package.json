{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "test": "jest",
    "build": "npx tsc",
    "start": "node build/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q build/server.js\""
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "verbose": false,
    "testMatch": [
      "/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "sequelize-cli": "^6.4.1",
    "ts-jest": "^29.1.1"
  }
}
